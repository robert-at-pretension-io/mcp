{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "YOUR_SUPABASE_ACCESS_TOKEN"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
      }
    }
  },
  "ai_provider": {
    "provider": "deepseek",
    "model": "deepseek-chat"
  },
  "timeouts": {
    "request": 120,
    "tool": 300
  }
}
